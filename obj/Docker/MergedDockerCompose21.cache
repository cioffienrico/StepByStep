services:
  stepbystep.database:
    environment:
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:9.5
    networks:
      stepbystep-network: null
    ports:
    - mode: ingress
      target: 4532
      published: 4532
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: pgdata
      target: /var/lib/postgresql/data
      volume: {}
  stepbystep.webapi:
    build:
      context: C:\Users\PC\Desktop\Visual Studio\Projetos\StepByStep
      dockerfile: StepByStep.Webapi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: StepByStep.WebApi
      target: base
    container_name: StepByStep.WebApi_1
    depends_on:
      stepbystep.database:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
      STEPBYSTEP_CONN: Host=stepbystep.database;Port=4532;Database=StepByStep;User
        Id=postgres;Password=postgres;
    image: stepbystepwebapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/StepByStep.WebApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      stepbystep-network: null
    ports:
    - mode: ingress
      target: 80
      published: 5000
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\PC\Desktop\Visual Studio\Projetos\StepByStep\StepByStep.Webapi
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\PC\Desktop\Visual Studio\Projetos\StepByStep
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\PC\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\PC\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\PC\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\PC\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  stepbystep-network:
    name: dockercompose11934771398908782014_stepbystep-network
volumes:
  pgdata:
    name: dockercompose11934771398908782014_pgdata